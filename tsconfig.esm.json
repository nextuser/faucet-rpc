{
  "compilerOptions": {
    "target": "es2020",
    "module": "node16",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    //"moduleResolution": "node16",
    "moduleSuffixes": [".js", ""]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}